{
    "name": "jeroennoten/laravel-ckeditor",
    "description": "Laravel CKEditor",
    "keywords": ["laravel", "ckeditor", "wysiwyg"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen Noten",
            "email": "jeroennoten@me.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JeroenNoten\\LaravelCkEditor\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:sjopet/laravel-package-helper.git"
        }
    ],
    "require": {
        "laravel/framework": "^5.6.0",
        "jeroennoten/laravel-package-helper": "^4.0.0",
        "league/flysystem": "^1.0",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "orchestra/testbench": "^3.6",
        "mockery/mockery": "^1.1",
        "hamcrest/hamcrest-php": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "laravel/browser-kit-testing": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeroenNoten\\LaravelCkEditor\\ServiceProvider"
            ]
        }
    }
}
